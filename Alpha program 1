var Sword = {
    x: 0,
    y: 0,
    animationFrame: 1,
    flip: false,
    flag: false,
    front: {attack: function(a,b,c)
        {
                if(Sword.animationFrame > 4.5 || Sword.animationFrame < 1 && !Sword.flip) //resets the sword's animation frames
                {
                Sword.animationFrame = 1;    
                }
                if(Sword.animationFrame >= 1.5 && Sword.animationFrame <= 2.45 && !Sword.flip)
                {
                pushMatrix();
                translate(a-scene.x+115,b-scene.y+63);
