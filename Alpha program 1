var drawSprite = function(sprite,x,y,w,h,flip,bw){
    noStroke();
    var currentSprite = sprites[sprite];
    var yarray = 0;
    var xarray = 0;
    var pyarray = 0;
    var count = 1;
    var last = currentSprite.imageData[yarray][xarray];
    var stop = true;
    pushMatrix();
    translate(x,y);
    for(yarray = 0; yarray < currentSprite.imageData.length; yarray++){for(xarray = 0; xarray < currentSprite.imageData[yarray].length; xarray++){
            if(!stop){
                if(last === currentSprite.imageData[yarray][xarray] && pyarray === yarray){
                    last = currentSprite.imageData[yarray][xarray];
                    count+=1;
                }else{
                    stop = true;
                }
            }
            if(stop){
            if(currentSprite.imageData[yarray][xarray] === 0){
                noFill();
                fill(0, 0, 0);
            }else{
                var a;
                if(!bw){
                a = colors.colorData[currentSprite.imageData[yarray][xarray]-1];
                }else{
                a = color(0, 0, 0);
                }
                fill(a);
                pushMatrix();
                if(!flip){
                    rect(w * xarray/currentSprite.imageData[yarray].length ,h *yarray/currentSprite.imageData.length, w * 1/currentSprite.imageData[yarray].length,h * 1/currentSprite.imageData.length);
                }else{
                    rect(-w * xarray/currentSprite.imageData[yarray].length+w,h *yarray/currentSprite.imageData.length,-w * 1/currentSprite.imageData[yarray].length,h * 1/currentSprite.imageData.length);
                }
                popMatrix();
            }
            count = 0;
            stop = true;
            last = currentSprite.imageData[yarray][xarray];
            }
        }
        pyarray = yarray+1;
    }
    popMatrix();
};
var MainCharacter = {
    mapX: 300, //if you want to start in another room you must 
    mapY: 300, //write in the value between where the room is found( for now)
    width: 24,
    height: 24,
    animationFrame: 1,//A sprite counter variable used to switch through different frames of sprites through different interactions
    activeStill: false,
    flip: false,
    flag: false,
    attack: false,
    
    front: {
        
        still: function(a,b,c)
        {
            drawSprite(0, a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),false);
        },
        walk: function(a,b,c,d,e)
        {
            if(a<7 && a > 1)
            {
            drawSprite(a, b || spriteX, c || spriteY, (spriteSize)*(d || scaled_size),  (spriteSize)*(d || scaled_size),e);
            }
            
        },
        animation: function(a,b,c)//uses sprite's walking frames and portays an illusion of it walking
        {
            if(MainCharacter.animationFrame < 2 || MainCharacter.animationFrame > 6.49)
                {
                    MainCharacter.animationFrame = 2;
                    
                }
            if(MainCharacter.animationFrame >= 6 && MainCharacter.animationFrame <= 6.49 && !MainCharacter.flip)
                {
                    MainCharacter.flip = true;
                    MainCharacter.animationFrame = 2;
                    
                }
                if(MainCharacter.animationFrame >= 6 && MainCharacter.animationFrame <= 6.49 && MainCharacter.flip)
                {
                    MainCharacter.flip = false;
                    MainCharacter.animationFrame = 2;
                }
                MainCharacter.animationFrame+= 0.15;
                //println(walkAnimation);
                //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(floor(MainCharacter.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),MainCharacter.flip);
            },
        attack: function(a,b,c)
        {
            if(MainCharacter.animationFrame < 7 || MainCharacter.animationFrame > 9.49)
                {
                    MainCharacter.animationFrame = 7;
                    
                }
            if(MainCharacter.animationFrame >= 9 && MainCharacter.animationFrame <= 9.45 && !MainCharacter.flip)
                {
                    MainCharacter.flag = true;
                }
                else{ MainCharacter.flag = false; }
                MainCharacter.animationFrame+= 0.15*0.5;
                //println(walkAnimation);
                //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(floor(MainCharacter.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size));
        }
        },
    side: {
        still: function(a,b,c,d)
        {
                            fill(0,0,0);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(12, a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size),d);

        },
        walk: function(a,b,c,d,e)
    {
        if(a<17 && a > 11)
        {
        drawSprite(a, b || spriteX, c || spriteY, (spriteSize)*(d || scaled_size),  (spriteSize)*(d || scaled_size),e);
        }
        
    },
    animation: function(a,b,c,d)//uses sprite's walking frames and portays an illusion of it walking
    {
        if(MainCharacter.animationFrame < 12 || MainCharacter.animationFrame > 16.49)
                {
                    MainCharacter.animationFrame = 12;
                    
                }
        if(MainCharacter.animationFrame >= 16 && MainCharacter.animationFrame <= 16.49 && !MainCharacter.flip)
            {
                MainCharacter.flip = true;
                MainCharacter.animationFrame = 12;
                
            }
            if(MainCharacter.animationFrame >= 16 && MainCharacter.animationFrame <= 16.49 && MainCharacter.flip)
            {
                //println(walkAnimation);
                MainCharacter.flip = false;
                MainCharacter.animationFrame = 12;
            }
            MainCharacter.animationFrame+= 0.15;
            //println(walkAnimation);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
        drawSprite(floor(MainCharacter.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),d);
        },
    attack: function(a,b,c,d)
    {
        if(MainCharacter.animationFrame < 17 || MainCharacter.animationFrame > 19.49)
            {
                MainCharacter.animationFrame = 19;
                
            }
        if(MainCharacter.animationFrame >= 17 && MainCharacter.animationFrame <= 17.49 && !MainCharacter.flip)
            {
                MainCharacter.flag = true;
                
            }
            else{MainCharacter.flag = false;}
            MainCharacter.animationFrame-= 0.15*0.7;
            //println(walkAnimation);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
        drawSprite(floor(MainCharacter.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),d);
    }
    },
    back: {
        still: function(a,b,c,d)
        {
                            fill(0,0,0);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(22, a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size),d);

        },
        walk: function(a,b,c,d,e)
    {
        if(a<37 && a > 29)
        {
        drawSprite(a, b || spriteX, c || spriteY, (spriteSize)*(d || scaled_size),  (spriteSize)*(d || scaled_size),e);
        }
        
    },
    animation: function(a,b,c,d)//uses sprite's walking frames and portays an illusion of it walking
    {
        if(MainCharacter.animationFrame < 21 || MainCharacter.animationFrame > 24.49)
                {
                    MainCharacter.animationFrame = 21;
                    
                }
        if(MainCharacter.animationFrame >= 24 && MainCharacter.animationFrame <= 24.49 && !MainCharacter.flip)
            {
                MainCharacter.flip = true;
                MainCharacter.animationFrame = 21;
                
            }
            if(MainCharacter.animationFrame >= 24 && MainCharacter.animationFrame <= 24.49 && MainCharacter.flip)
            {
                //println(walkAnimation);
                MainCharacter.flip = false;
                MainCharacter.animationFrame = 21;
            }
            MainCharacter.animationFrame+= 0.15;
            
            //println(walkAnimation);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            
        drawSprite(floor(MainCharacter.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),d);
        
        },
        attack: function(a,b,c,d)
    {
        if(MainCharacter.animationFrame < 25 || MainCharacter.animationFrame > 28.49)
            {
                MainCharacter.animationFrame = 28;
                
            }
        if(MainCharacter.animationFrame >= 25 && MainCharacter.animationFrame <= 25.49 && !MainCharacter.flip)
            {
                MainCharacter.flag = true;
                
            }
            else{MainCharacter.flag = false;}
            MainCharacter.animationFrame-= 0.15*0.9;
            //println(walkAnimation);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
        drawSprite(floor(MainCharacter.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),d);
    }
    },
    hurtbox: {},
};//Main character assortment and interactions
var scene = {
    sizeX: 0, //current room width (is called and assigned a value later down the line)
    sizeY: 0, // same thing here
    
    //current camera frame position
    x: GameMapX[mapY][mapX]/width,
    y: GameMapY[mapY][mapX]/height,
collision: { //collision detections 
    RightX: 0, //(is called and assigned a value later down the line)
    TopY: 0, // same thing here
    LeftX: 0, // same thing here
    BottomY: 0, // same thing here
    width: 10, //width of detection range
    
    active: function(a,b) //active detection function (uses mapX & mapY)
    {
        if(Stationary) //stationary camera logic (sees how will the camera react to a room bigger than the canvas width and height itself.
        { 
            
            if(MainCharacter.mapX-scene.collision.LeftX > width/2 && 
            MainCharacter.mapX-scene.collision.RightX < -width/2)
           {
           
            //println("it works");
            scene.x = MainCharacter.mapX-width/2;
           
           }
           if( 
            (MainCharacter.mapY >scene.collision.TopY+height/2) && 
            (MainCharacter.mapY <scene.collision.BottomY- height/2))
           {
           
            //println("it works");
            scene.y = MainCharacter.mapY-height/2;
        }
        }
        /* (This was part of what was going to be a bigger map with several different rooms, and interactions between themthe map however was shortened due to time capacity
        
        //Right Scroll detection logic
        if(MainCharacter.mapX-(scene.collision.RightX)>=30)
        {
            lastCollision.x = scene.sizeX; //for debugging purposes
            Stationary = false;
            
                SceneMove = true;
                if(SceneMove)
                {
                scene.x+= cos(1)*8;
                //println("moving");
                    if(scene.x >= scene.collision.RightX)
                    {
                        SceneMove = false;
                        mapX++;
                        if(scene.sizeX > width || scene.sizeY > height)
                        {
                        Stationary = true;
                        }
                    }
                }
        }
        
        //Bottom Scroll detection logic
        if(MainCharacter.mapY-(scene.collision.BottomY)>=30)
        {
            lastCollision.y = scene.sizeY; //for debugging purposes
            Stationary = false;
            
                SceneMove = true;
                println(SceneMove);
                if(SceneMove)
                {
                scene.y+= cos(1)*8;
                //println("moving");
                    if(scene.y >= scene.collision.BottomY)
                    {
                        SceneMove = false;
                        mapY++;
                        
                        if(scene.sizeX > width || scene.sizeY > height)
                        {
                            println("big room");
                        Stationary = true;
                        }
                    }
                }
        }
        //Top Scroll detection
        if((MainCharacter.mapY-scene.collision.TopY)<=-35 && mapY > 0)
        {
            lastCollision.y = scene.sizeY; //for debugging purposes
            Stationary = false;
                SceneMove = true;
                if(SceneMove)
                {
                    
                scene.y-= cos(1)*8;
                //println("top scroll");
                    if(scene.y <=scene.collision.TopY-height)
                    {
                        mapY--;
                        SceneMove = false;
                        
                        if(GameMapX[mapY][mapX] > width ||GameMapY[mapY][mapX] > height)
                        {
                        Stationary = true;
                        }
                        
                    }
                }
        }
        //Left Scroll detection
        if((MainCharacter.mapX-scene.collision.LeftX)<=-35 && mapX > 0)
        {
            lastCollision.x = scene.sizeX; //for debugging purposes
            Stationary = false;
                SceneMove = true;
                if(SceneMove)
                {
                    
                scene.x-= cos(1)*8;
                //println("moving");
                    if(scene.x <=scene.collision.LeftX-width)
                    {
                        mapX--;
                        SceneMove = false;
                        
                        if(GameMapX[mapY][mapX] > width ||GameMapY[mapY][mapX] > height)
                        {
                        Stationary = true;
                        }
                    }
                }
    }*/   },
}}; //Properties and logic of the camera system
var Ghost = {
    mapX: 300, //if you want to start in another room you must 
    mapY: 300, //write in the value between where the room is found( for now)
    width: 24,
    height: 24,
    animationFrame: 1,//A sprite counter variable used to switch through different frames of sprites through different interactions
    activeStill: false,
    flip: false,
    flag: false,
    frame: 0,
    health: 2,
    death: false,
    healthBar: function(a,b)
    {
        if (Ghost.health === 2)
        {
            drawSprite(76,a-scene.x,b-scene.y,24*3,12);
        }
        else if (Ghost.health === 1)
        {
            drawSprite(81,a-scene.x,b-scene.y,24*4,24);
        }
        else if (Ghost.health === 0)
        {
            drawSprite(86,-scene.x,b-scene.y,24*4,24);
            Ghost.death = true;
        }
    },
    front: {
        
        still: function(a,b,c)
        {
            drawSprite(29, a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),false);
        },
        walk: function(a,b,c,d,e)
        {
            if(a<34 && a > 30)
            {
            drawSprite(a, b || spriteX, c || spriteY, (spriteSize)*(d || scaled_size),  (spriteSize)*(d || scaled_size),e);
            }
            
        },
        animation: function(a,b,c)//uses sprite's walking frames and portays an illusion of it walking
        {
            if(Ghost.animationFrame < 30 || Ghost.animationFrame > 34.49)
                {
                    Ghost.animationFrame = 30;
                    
                }
            if(Ghost.animationFrame >= 34 && Ghost.animationFrame <= 34.49 && !Ghost.flip)
                {
                    Ghost.flip = true;
                    Ghost.animationFrame = 30;
                    
                }
                if(Ghost.animationFrame >= 34 && Ghost.animationFrame <= 34.49 && Ghost.flip)
                {
                    Ghost.flip = false;
                    Ghost.animationFrame = 30;
                }
                Ghost.animationFrame+= 0.15;
                //println(walkAnimation);
                //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(floor(Ghost.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),Ghost.flip);
            },
        attack: function(a,b,c)
        {
            if(Ghost.animationFrame < 35 || Ghost.animationFrame > 38.49)
                {
                    Ghost.animationFrame = 35;
                    
                }
            if(Ghost.animationFrame >= 38 && Ghost.animationFrame <= 38.45 && !Ghost.flip)
                {
                    Ghost.flag = true;
                }
                else{ Ghost.flag = false; }
                Ghost.animationFrame+= 0.15*0.5;
                //println(walkAnimation);
                //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(floor(Ghost.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size));
        }
        },
    side: {
        still: function(a,b,c,d)
        {
                            fill(0,0,0);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
            drawSprite(39, a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size),d);

        },
        walk: function(a,b,c,d,e) //used for debugging
    {
        if(a<44 && a > 40)
        {
        drawSprite(a, b || spriteX, c || spriteY, (spriteSize)*(d || scaled_size),  (spriteSize)*(d || scaled_size),e);
        }
        
    },
    animation: function(a,b,c,d)//uses sprite's walking frames and portays an illusion of it walking
    {
        if(Ghost.animationFrame < 40 || Ghost.animationFrame > 44.49)
                {
                    Ghost.animationFrame = 40;
                    
                }
        if(Ghost.animationFrame >= 44 && Ghost.animationFrame <= 44.49 && !Ghost.flip)
            {
                Ghost.flip = true;
                Ghost.animationFrame = 40;
                
            }
            if(Ghost.animationFrame >= 44 && Ghost.animationFrame <= 44.49 && Ghost.flip)
            {
                //println(walkAnimation);
                Ghost.flip = false;
                Ghost.animationFrame = 40;
            }
            Ghost.animationFrame+= 0.15;
            //println(walkAnimation);
            //rect(a,b,(spriteSize)*(c || scaled_size),(spriteSize)*(c || scaled_size));
        drawSprite(floor(Ghost.animationFrame), a || spriteX, b || spriteY, (spriteSize)*(c || scaled_size),  (spriteSize)*(c || scaled_size),d);
        },
    attack: function(a,b,c,d)
    {
        if(Ghost.animationFrame < 45 || Ghost.animationFrame > 46.49)
            {
                Ghost.animationFrame = 46;
                
            }
        if(Ghost.animationFrame >= 45 && Ghost.animationFrame <= 45.49 && !Ghost.flip)
            {
                Ghost.flag = true;
                
            }
            else{Ghost.flag = false;}
            Ghost.animationFrame-= 0.15*0.7;
