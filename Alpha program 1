{
            if(coin_temp >= 1 && coin_temp <= 5.49)
                {
                 drawSprite(70,120,5,59,61);
                }
                 if(coin_temp >= 5.5 && coin_temp <= 8.49)
                {
                    drawSprite(71,130,10,49,51);
                }
                if(coin_temp >= 8.5 && coin_temp <= 12.49)
                {
                    drawSprite(72,120,5,59,61);
                }
                if(coin_temp >= 12.5 && coin_temp <= 16.49)
                {
                    drawSprite(71,122,10,49,51,true);
                }
                if(coin_temp >= 16.5)
                {
                    coin_temp = 1;
                }
                coin_temp+= 0.5;
};
