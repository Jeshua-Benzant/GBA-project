var Ghost_AI = function(a,b,c)
{
    this.x = a+Ghost.mapX;
    this.y = b+Ghost.mapY;
    this.size = c;
    this.done = false;
    this.still = false;
    this.health = Ghost.health*c/4;
    this.hit = function(a)
    {
        var temp = 10+a;
        var flag = false;
        if (temp > 0)
        {
            
            temp -= cos(1)*2/5;
        }
        else{  }
    };
    
           //println(Ghost.frame);
    //println(this.y > (MainCharacter.mapY-scene.y+50));
    //println(dist(Ghost.mapX,this.y,MainCharacter.mapX,MainCharacter.mapY));
     
};
Ghost_AI.prototype.draw = function() {
    if(this.size > 0)
    {
    Ghost.healthBar(this.x,this.y-10);
    if(dist(this.x,this.y,MainCharacter.mapX,MainCharacter.mapY) > 10) //function for Ghost to follow main Character
    {

        //println("good");
        if(this.y-MainCharacter.mapY >100 )
        {
            println("working");
            Ghost.back.animation(this.x-scene.x,this.y-scene.y,this.size);
            Ghost.frame = 1;
         this.y-= cos(1)*3;   
        }
        else if (this.y - MainCharacter.mapY <-100)
        {
            //println("working");
            Ghost.front.animation(this.x-scene.x,this.y-scene.y,this.size);
            this.y += cos(1)*3;
            Ghost.frame = 2;
        }
        if(this.x-MainCharacter.mapX >100 )
        {
            //println("working");
            Ghost.side.animation(this.x-scene.x,this.y-scene.y,this.size);
         this.x-= cos(1)*3;
         Ghost.frame = 3;
        }
        else if (this.x - MainCharacter.mapX <-100)
        {
            //println("working");
            Ghost.side.animation(this.x-scene.x,this.y-scene.y,this.size,true);
            this.x+= cos(1)*3;
            Ghost.frame = 4;
        }
        else{this.still = true;}
    }
        if(this.still) //if the ghost get close enough to the player it will stand still and prepare to attack
        {
            if (Ghost.frame === 1)
            {
                Ghost.back.animation(this.x-scene.x,this.y-scene.y,this.size);
            }
            else if (Ghost.frame === 2)
            {
                Ghost.front.animation(this.x-scene.x,this.y-scene.y,this.size);
            }
            else if (Ghost.frame === 3)
            {
                Ghost.side.animation(this.x-scene.x,this.y-scene.y,this.size);
            }
            else if (Ghost.frame === 4)
            {
                Ghost.side.animation(this.x-scene.x,this.y-scene.y,this.size,true);
            }
            else{this.done = true;}
        }
        if (this.done) {/*Ghost.front.animation(this.x-scene.x,this.y-scene.y,this.size,true);*/
            }
            if (MainCharacter.flag)
           { 
         this.done = false;
            if (Ghost.frame === 1 && MainCharacter.flag && !this.hit())
            {
                this.y += 35;
                Ghost.health--;
                MainCharacter.flag = false;
                this.hit(50);
            }
            else if (Ghost.frame === 2)
            {
                this.y -= 15;
                Ghost.health--;
                MainCharacter.flag = false;
            }
            else if (Ghost.frame === 3)
            {
                this.x += 15;
                Ghost.health--;
                MainCharacter.flag = false;
            }
            else if (Ghost.frame === 4)
            {
                this.x -= 15;
                Ghost.health--;
                MainCharacter.flag = false;
            }
            else{this.done = true;}
        }
        if(Ghost.health <= 0)
        {
            
            this.size -= cos(1)/4;
        }}
};
var Ghost1 = new Ghost_AI(100,10,5);
var Ghost2 = new Ghost_AI(-100,0,4);

fill(255, 0, 0); //rectangles to distinguish the rooms
    rect(-scene.x,0-scene.y,GameMapX[0][0],GameMapY[0][0]);

    collisions(); //collisions of the rooms
    scene.collision.active(mapX,mapY); //call function for active collisions

    fill(0,0,0);
    MainCharacter.sprite(48,48);

    Ghost1.draw();
    Ghost2.draw();
    
    rect(100-scene.x,800-scene.y,100,100);
    
    UI();
}};
currentScene = 0;
