MainCharacter.sprite = function(){
    var wasd = function() //variable to organize movement and make it easier to access
{if (!SceneMove) //wont move if there is a room transition (old Nintendo Gameboy effect)
{
    //movement and borders
        if(keys[68] && MainCharacter.mapX < limit.x())
    {//println("it d");
        MainCharacter.mapX+= 4; //moves Character coordinates by +X
        frame = 68;
        if(frame === 68)
        { //specific animation that plays at this key press
        MainCharacter.side.animation(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4,true);
        }
    }
    else if(keys[65] && MainCharacter.mapX > 0)
    {//println("it a");
        MainCharacter.mapX-= 4; //moves Character coordinates by -X
        frame = 65;
        if(frame === 65)
         { //specific animation that plays at this key press
        MainCharacter.side.animation(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
         }
    }
    
    else if(keys[87] && MainCharacter.mapY > 0)
    {//println("it w");
        MainCharacter.mapY-= 4; //moves Character coordinates by -Y
        frame = 87;
        if(frame === 87){ //specific animation that plays at this key press
        MainCharacter.back.animation(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        }
    }
    
    else if(keys[83] && MainCharacter.mapY < limit.y())
    {//println("it s");
        MainCharacter.mapY+= 4; //moves Character coordinates by +Y
        frame = 83; 
        if(frame === 83)
        { //specific animation that plays at this key press
        MainCharacter.front.animation(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        }
    }
    else{key_temp = false;
    MainCharacter.activeStill = true;
    }
} //makes sure no sprites overlap
   
   //this segment here remebers which key frame was pressed last and keeps that frame up until the user moves again.
  if (!key_temp && !keys[80])
    {
        if(frame === 68) 
        {
        MainCharacter.side.still(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4,true);
        }
        else if (frame === 65) {
        MainCharacter.side.still(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        }
        else if (frame === 87) 
        {
        MainCharacter.back.still(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        }
        else if (frame === 83) 
        {
        MainCharacter.front.still(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        }
        else if (frame === 0)
        {
            MainCharacter.front.still(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        }
    }
   
    //attack animation organization
    if(keys[80] && !key_temp && MainCharacter.activeStill)
    {
        MainCharacter.attack = true;
        if(frame === 68) 
        {
            
            rect(MainCharacter.mapX-scene.x+60,MainCharacter.mapY-scene.y+50,80,30);
        MainCharacter.side.attack(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4,true);
        Sword.leftSide.attack(MainCharacter.mapX,MainCharacter.mapY);
        }
        else if (frame === 65) {
            rect(MainCharacter.mapX-scene.x+35,MainCharacter.mapY-scene.y+50,-80,30);
        MainCharacter.side.attack(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        Sword.rightSide.attack(MainCharacter.mapX,MainCharacter.mapY);
        }
        else if (frame === 87) 
        {
            rect(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,60,70);
            Sword.back.attack(MainCharacter.mapX,MainCharacter.mapY);
        MainCharacter.back.attack(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        
        }
        else if (frame === 83) 
        {
            
            
            rect(MainCharacter.mapX-scene.x+60,MainCharacter.mapY-scene.y+50,80,90);
            Sword.front.attack(MainCharacter.mapX,MainCharacter.mapY);
        MainCharacter.front.attack(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);
        fill(0,0,0);
        
        }
        
        if (frame === 0)
        {
            MainCharacter.front.still(MainCharacter.mapX-scene.x,MainCharacter.mapY-scene.y,4);}
            
    }
    else{MainCharacter.attack = false;}
};

wasd();
 
    
}; //player logic, movement, and movement animation

var collisions = function() //collisions and checks for any differences in the mapX and mapY values
{
    scene.sizeX  = GameMapX[mapY][mapX];
    scene.sizeY  = GameMapY[mapY][mapX];
    scene.collision.RightX = (Actual.x()-scene.collision.width) || 0;
scene.collision.TopY = (Actual.y()-scene.sizeY-scene.collision.width) || 0;
scene.collision.LeftX = (Actual.x()-scene.sizeX-scene.collision.width) || 0;
scene.collision.BottomY = (Actual.y()-scene.collision.width) || 0;
};
